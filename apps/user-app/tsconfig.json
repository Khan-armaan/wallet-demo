{
  "extends": "@repo/typescript-config/nextjs.json",
  "compilerOptions": {
    "noImplicitAny": false,
    "plugins": [
      {
        "name": "next"
      }
    ], "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "next.config.js",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
     "types/**/*.d.ts"
  ],
  "exclude": ["node_modules"]
}
